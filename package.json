{
  "name": "gps_tracker",
  "version": "0.0.1",
  "description": "GPS Devices Tracking IOT",
  "author": "NighaTech Global Pvt LTd",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "build:serverless": "sls package",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:serverless": "sls offline",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:migrate": "prisma migrate deploy",
    "deploy": "bash deploy.sh",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:staging": "sls deploy --stage staging",
    "deploy:prod": "sls deploy --stage prod",
    "remove": "sls remove",
    "logs": "sls logs -f",
     "invoke": "sls invoke -f",
     "serverless:info": "sls info",
     "validate:migration": "node validate-migration.js",
     "prevalidate:migration": "npm install",
     "postinstall": "prisma generate"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",

    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.3",
    "@prisma/client": "^6.11.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.5",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "express": "^4.18.2",
    "class-validator": "^0.14.2",
    "mongodb": "^6.17.0",
